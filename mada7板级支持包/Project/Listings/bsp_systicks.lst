              //  TCON|=0x10;
              }
              
              void delay(unsigned int time)
              {
                while(time--);
              }
              
              void delay_ms(unsigned int timer_ms)
              {
                Systicks_T0_Init();
                while(millisecond<timer_ms*4);
              //  TCON&=0xCF;
                TR0=0;
                millisecond=0;
              }
              
              void delay_s(unsigned int timer_s)
              {
                Systicks_T0_Init();
                while(second<timer_s)
                {
                  if(millisecond==4000)
                  {
                    second++;
                    millisecond=0;
                  }
                }
              //  TCON&=0xCF;
                TR0=0;
                millisecond=0;
                second=0;
              }
              
              
